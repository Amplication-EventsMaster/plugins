datasource db {
  provider = "postgresql"
  url      = env("DB_URL")
}

generator client {
  provider = "prisma-client-js"
}

model PluginLink {
  createdAt  DateTime @default(now())
  githubLink String?
  id         String   @id @default(cuid())
  npmLink    String?
  updatedAt  DateTime @updatedAt
}

model Plugin {
  createdAt   DateTime @default(now())
  description String?
  id          String   @id @default(cuid())
  pluginId    String?
  settings    Json?
  updatedAt   DateTime @updatedAt
}

model PluginSetting {
  createdAt DateTime @default(now())
  id        String   @id @default(cuid())
  key       String?
  updatedAt DateTime @updatedAt
  value     String?
}

model PluginIcon {
  createdAt DateTime @default(now())
  id        String   @id @default(cuid())
  updatedAt DateTime @updatedAt
  url       String?
}
